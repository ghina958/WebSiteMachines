<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Dashboard</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/WebCore/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bootstrap 4 -->
    <link rel="stylesheet" href="~/WebCore/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="~/WebCore/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="~/WebCore/plugins/jqvmap/jqvmap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/WebCore/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/WebCore/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/WebCore/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="~/WebCore/plugins/summernote/summernote-bs4.min.css">
</head>

<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

        <!-- Preloader -->
        <div class="preloader flex-column justify-content-center align-items-center">
            <img class="animation__shake" src="~/WebCore/dist/img/AdminLTELogo.png" alt="AdminLTELogo" height="60" width="60">
        </div>

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>

            </ul>
            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="/Home" class="nav-link">
                        <i class="fas fa-store"></i>
                        Public Store
                    </a>
                </li>
            </ul>
        </nav>

        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <div class="sidebar">
                <!-- Admin Photo in SidNavBar -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="pull-left logo-outer">
                        <div class="logo"><a href="index.html"><img src="~/images/logo/logo.png" alt="" title="Beirut Finance"></a></div>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                       
                        <li class="nav-item menu-open">
                            <a href="/DashboardAdmin/Index" class="nav-link">
                                <i class="nav-icon fas fa-tachometer-alt"></i>
                                <p>
                                    Dashboard
                                </p>
                            </a>
                        </li>
                       
                        <li class="nav-item">
                            <a class="nav-link" href="/Category/Index">
                                <i class="nav-icon fas fa-th"></i>
                                <p>
                                    Categories
                                </p>
                            </a>
                        </li>
                      
                        <li class="nav-item">
                            <a class="nav-link" href="/Product/Index">
                                <i class='nav-icon  fas fa-shopping-bag' style='font-size:18px'></i>
                                <p>
                                    Products
                                </p>
                            </a>
                        </li>

                        <li class="nav-item menu-open">
                            <a href="/Service/Index" class="nav-link ">
                                <i class='nav-icon  fas fa-shopping-bag' style='font-size:18px'></i>
                                <p>
                                    Services
                                </p>
                            </a>
                        </li>

                        <li class="nav-item menu-open">
                            <a href="/ContactInfo/Index" class="nav-link ">
                                <i class='nav-icon  fas fa-shopping-bag' style='font-size:18px'></i>
                                <p>
                                    Contact Info
                                </p>
                            </a>
                        </li>

                        <li class="nav-item menu-open">
                            <a href="/AboutUs/Index" class="nav-link ">
                                <i class='nav-icon  fas fa-shopping-bag' style='font-size:18px'></i>
                                <p>
                                    About Us
                                </p>
                            </a>
                        </li>

                    </ul>
                </nav>
            </div>
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-12">
                            <ol class="breadcrumb float-sm-left">
                                <li class="breadcrumb-item"><a href="/DashboardAdmin/Index">Home Page</a></li>

                                @if (ViewBag.BreadCrumbFirstItem != null)
                                {
                                    <li class="breadcrumb-item">
                                        <a href="@(ViewBag.BreadCrumbFirstItemLink)">
                                            @(
                                                ViewBag.BreadCrumbFirstItem
                                                )
                                        </a>
                                    </li>
                                }
                                @*   <li class="breadcrumb-item"><a href="/dashboard">Anasayfa</a></li>*@ 

                                @if (ViewBag.BreadCrumbSecondItem != null)
                                {
                                    <li class="breadcrumb-item active">
                                        <a href="@(ViewBag.BreadCrumbSecondItemLink)">
                                            @(
                                                ViewBag.BreadCrumbSecondItem
                                                )
                                        </a>
                                    </li>
                                }
                            </ol>
                        </div><!-- /.col -->
                        <div class="col-sm-12">
                            <h1 class="m-0">@(ViewBag.Title)</h1>
                        </div><!-- /.col -->
                      
                    </div><!-- /.row -->
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content-header -->
            <!-- Main content -->
            <section class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div><!-- /.container-fluid -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>
               
            </strong>
            All rights reserved.
            <div class="float-right d-none d-sm-inline-block">
               @* <b>Version</b> 3.2.0*@
            </div>
        </footer>

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/WebCore/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/WebCore/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- Bootstrap 4 -->
    <script src="~/WebCore/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- ChartJS -->
    <script src="~/WebCore/plugins/chart.js/Chart.min.js"></script>
    <!-- Sparkline -->
    <script src="~/WebCore/plugins/sparklines/sparkline.js"></script>
    <!-- JQVMap -->
    <script src="~/WebCore/plugins/jqvmap/jquery.vmap.min.js"></script>
    <script src="~/WebCore/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/WebCore/plugins/jquery-knob/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/WebCore/plugins/moment/moment.min.js"></script>
    <script src="~/WebCore/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/WebCore/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Summernote -->
    <script src="~/WebCore/plugins/summernote/summernote-bs4.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/WebCore/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/WebCore/dist/js/adminlte.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/WebCore/dist/js/demo.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="~/WebCore/dist/js/pages/dashboard.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
